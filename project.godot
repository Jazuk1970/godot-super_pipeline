[application]

config/name="Godot-super Pipeline"
config/description="A version of super Pipeline by TaskSet on the C64"
run/main_scene="res://scenes/Game/Game.tscn"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

file_handling="*res://autoloads/file_handling.gd"
utility="*res://autoloads/utility.gd"
globals="*res://autoloads/globals.gd"
AudioManager="*res://autoloads/AudioManager.tscn"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/narrowing_conversion=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/integer_division=false

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/size/test_width=960
window/size/test_height=540
window/stretch/mode="2d"
window/stretch/aspect="keep"

